{
  "manifest_version": 2,
  "name": "StoriesWeLove",
  "author": "FoxInFlame",
  "description": "A Material Design inspired WattPad client.",
  "version": "0.8",
  "icons": {
    "32": "32.png",
    "64": "64.png",
    "128": "128.png",
    "256": "256.png"
  },
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "libraries/jQuery/jquery-2.2.3.min.js",
        "background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "alwaysOnTopWindows",
    "<all_urls>"
  ]
}